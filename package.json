  "bugs": {
    "url": "https://github.com/shynome/node-v2ray-proto/issues"
  },
  "homepage": "https://github.com/shynome/node-v2ray-proto#readme",
  "devDependencies": {
    "@grpc/grpc-js": "^0.6.15",
    "glob": "^7.1.6",
    "protobufjs": "^6.8.8",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.7.4"
  }
}