  "main": "core_pb.js",
  "types": "core_pb.d.ts",
  "scripts": {
    "pregen:pb": "rm -f core_pb.js core_pb.d.ts",
    "gen:pb": "pbjs -t static-module -w commonjs $(node -pe \"require('glob').sync('v2ray.com/core/**/*.proto').join(' ')\") | tee core_pb.js | pbts -o core_pb.d.ts -"
